#include <stdio.h>

int main() {
    int size =3;
    int arr[size];
  printf("Enter element in array:");
     for (int i = 0; i < size; i++)
      {
         scanf("%d",&arr[i]);
      }
       printf("Before insertion:\n" );
    for (int i = 0; i < size; i++) {
        printf("%d ", arr[i]);
    }
      size++;
 
    arr[size-1]=100;
      printf("\nAfter insertion:\n" );
    for (int i = 0; i < size; i++) {
        printf("%d ", arr[i]);
    }
    

 return 0;
}

 
