#include <stdio.h>

int main() {
    
    int a[5] = {1, 2, 3, 4, 5};
    int b[5] = {6,7,8,9,10};
    int c[10],i,j=0;
    
     

    for (i = 0; i < 5; i++)  
      {
            c[i]=a[i];
      }
      for (i = 5; i < 10; i++) 
      {
             c[i]=b[j];
             j++;
      }
    
    
    for (i = 0; i < 10; i++) 
    {
       printf("%d\t",c[i]); 
        
    }

    return 0;
}
